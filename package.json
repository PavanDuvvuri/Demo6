    "@wdio/cucumber-framework": "6.0.13",
    "@wdio/local-runner": "6.0.13",
    "@wdio/spec-reporter": "5.22.4",
    "@wdio/sync": "6.0.12",
    "chromedriver": "80.0.2",
    "cucumber-html-reporter": "5.2.0",
    "eslint": "^6.8.0",
    "expect-webdriverio": "3.1.3",
    "selenium-standalone": "6.17.0",
